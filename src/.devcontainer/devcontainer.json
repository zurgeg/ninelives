{
    "image": "ghcr.io/playdate-projects/playdate-docker:main",
    "customizations": {
      "vscode": {
        "extensions": [
        "midouest.playdate-debug",
        "sumneko.lua"
      ]
      }
    },
    "remoteUser": "playdate-sdk",
    "features": {
      "git": "latest",
      "github-cli": "latest"
    },
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash (2)", 
      "terminal.integrated.profiles.linux": {
          "bash": {
              "path": "/bin/bash"
          }
      }
    }
  }